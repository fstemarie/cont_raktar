{
	"deploy.reloaded": {
		"alwaysShowWorkspaceNameInPopups": true,
		"packages": [
			{
				"name": "docker-compose scripts",
				"description": "Mes script de docker-compose",
				"files": [
					"**/docker-compose.yml",
					"**/docker-compose.yaml"
				]
			}
		],
		
		"targets": [
			{
				"type": "sftp",
				"name": "Raktar - docker-compose",
				"description": "Pour deployer mes scripts docker-compose",

				"dir": "/home/francois/docker-compose",
				"host": "raktar.local",
				"port": 22,
				"user": "francois",
				"agent": "pageant",
				"modes":{
					"/**/*.yml": 664,
					"/**/*.yaml": 664,
				}
			}
		],

		"buttons": [
			{
				"text": "Push to Raktar",
				"command": "extension.deploy.reloaded.pullPackgeFiles",
				"arguments": [ "docker-compose scripts", "Raktar - docker-compose"]
			}
		]
	}
}